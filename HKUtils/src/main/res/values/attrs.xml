<?xml version="1.0" encoding="utf-8"?>
<resources>
	<attr name="itemLayout" format="reference"/>

	<declare-styleable name="HKList">
		<attr name="emptyMessage" format="string"/>
		<attr name="emptyDescription" format="string"/>
		<attr name="emptyImage" format="reference"/>
		<attr name="itemLayout"/>
	</declare-styleable>

	<declare-styleable name="DropDown">
		<attr name="itemLayout" />
	</declare-styleable>

	<declare-styleable name="SelectableEditText">
		<attr name="itemLayout" />
	</declare-styleable>

	<declare-styleable name="ExpandableLayout">
		<attr name="expanded" format="boolean"/>
		<attr name="duration" format="integer"/>
	</declare-styleable>

	<declare-styleable name="EventCalendarView">
		<attr name="dayLayout" format="reference"/>
	</declare-styleable>

	<declare-styleable name="CircularProgressButton">
		<attr name="progressBarWidth" format="dimension"/>
		<attr name="progressColor" format="color"/>
		<attr name="progressBackgroundColor" format="color"/>
		<attr name="buttonColor" format="color"/>
		<attr name="shadowColor" format="color"/>
		<attr name="elevation" format="dimension"/>
		<attr name="android:duration" format="integer"/>
		<attr name="maxScale" format="float"/>
	</declare-styleable>
</resources>